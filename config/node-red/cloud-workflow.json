        }
    },
    {
        "id": "cloud_comment_01",
        "type": "comment",
        "z": "cloud_workflow_tab_01",
        "name": "Cloud → Local: Light Commands (rv/lights/N/command → CAN outbound)",
        "info": "Receives light toggle commands from the cloud broker and converts them to CAN bus messages sent to can/outbound on the local broker.",
        "x": 310,
        "y": 40,
        "wires": []
    },
    {
        "id": "cloud_light_cmd_in_1",
        "type": "mqtt in",
        "z": "cloud_workflow_tab_01",
        "name": "Cloud Light1Cmd",
        "topic": "rv/lights/1/command",
        "qos": "2",
        "datatype": "auto-detect",
        "broker": "cloud_mqtt_broker_01",
